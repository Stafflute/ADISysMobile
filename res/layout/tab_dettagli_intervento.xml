<?xml version="1.0" encoding="utf-8"?>
<merge>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="horizontal"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:id="@+id/tabDettagliInterventoContent">

    <Space
            android:layout_width="8px"
            android:layout_height="20px"
            android:id="@+id/space1"/>
    <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/scrollView" android:fillViewport="true">
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Intervento"
                    android:id="@+id/interventoText" android:layout_gravity="left|center_vertical" android:textSize="25dp"/>
            <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                          android:layout_height="wrap_content" android:gravity="top" android:id="@+id/gruppoIntervento">
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent" android:layout_marginLeft="12dp">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content" android:gravity="top">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="ID"
                                android:id="@+id/idText" android:layout_gravity="center_horizontal|top"/>
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=": "
                                android:id="@+id/textView" android:layout_gravity="center_horizontal|top"/>
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:id="@+id/idValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="CittÃ "
                                  android:id="@+id/cittaText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView2" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/cittaValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout1">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cap"
                                  android:id="@+id/capText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView3" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/capValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout2">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Indirizzo"
                                  android:id="@+id/indirizzoText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView6" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/indirizzoValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout3">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Data"
                                  android:id="@+id/dataText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView9" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/dataValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout4">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Ora"
                                  android:id="@+id/oraText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView12" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/oraValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Paziente"
                      android:id="@+id/pazienteText" android:layout_gravity="left|center_vertical" android:textSize="20dp"
                      android:paddingTop="6dp"/>
            <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                          android:layout_height="wrap_content" android:gravity="top"
                          android:id="@+id/gruppoPaziente">
                <LinearLayout android:orientation="vertical" android:layout_width="fill_parent"
                              android:layout_height="fill_parent" android:layout_marginLeft="12dp">
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout5">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="ID"
                                  android:id="@+id/idPazienteText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView4" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/idPazienteValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout6">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Nome"
                                  android:id="@+id/nomePazienteText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView8" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/nomePazienteValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout7">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cognome"
                                  android:id="@+id/cognomePazienteText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView13" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/cognomePazienteValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout8">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Data"
                                  android:id="@+id/dataPazienteText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView16" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/dataPazienteValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="vertical" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout9">
                        <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Numeri telefonici"
                                android:id="@+id/rubricaText" android:paddingTop="3dp"/>
                        <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" android:layout_marginLeft="9dp">
                            <LinearLayout android:orientation="vertical" android:layout_width="fill_parent"
                                          android:layout_height="wrap_content"
                                    android:id="@+id/rubricaList"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Infermiere"
                      android:id="@+id/infermiereText" android:layout_gravity="left|center_vertical" android:textSize="20dp"
                      android:paddingTop="6dp"/>
            <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                          android:layout_height="wrap_content" android:gravity="top"
                          android:id="@+id/gruppoInfermiere">
                <LinearLayout android:orientation="vertical" android:layout_width="fill_parent"
                              android:layout_height="fill_parent" android:layout_marginLeft="12dp">
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout10">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="ID"
                                  android:id="@+id/idInfermiereText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView7" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/idInfermiereValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout12">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Nome"
                                  android:id="@+id/nomeInfermiereText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView18" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/nomeInfermiereValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                    <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent"
                                  android:layout_height="wrap_content" android:gravity="top" android:id="@+id/linearLayout11">
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cognome"
                                  android:id="@+id/cognomeInfermiereText" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text=": "
                                  android:id="@+id/textView14" android:layout_gravity="center_horizontal|top"/>
                        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                                  android:id="@+id/cognomeInfermiereValue" android:layout_gravity="center_horizontal|top"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
</merge>